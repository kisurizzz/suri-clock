    "expo": "~53.0.0",
    "expo-location": "~18.1.5",
    "expo-status-bar": "~2.2.3",
    "firebase": "^10.3.1",
    "metro": "^0.82.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-react": "^7.27.1",
    "@react-native-community/cli": "^18.0.0"
  },
